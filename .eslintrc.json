{
    "extends": "airbnb",
    "plugins": [
        "react",
        "react-native",
        "jsx-a11y",
        "import",
        "jest"
    ],
    "env": {
        "jest/globals": true
    },
    "rules": {
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-filename-extension": ["error", { "extensions": [".js"] }],
        "no-underscore-dangle": ["error", { "allow": ["__DEV__"] }]
    }
}
